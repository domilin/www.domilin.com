{
  "name": "www.domilin.com-backend",
  "version": "0.1.0",
  "description": "www.domilin.com-backend",
  "repository": {
    "type": "git",
    "url": "https://github.com/domilin/www.domilin.com-backend"
  },
  "author": "domilin",
  "license": "MIT",
  "scripts": {
    "pm2": "cross-env NODE_ENV=production pm2 restart ../api.domilin.com.json",
    "start": "cross-env NODE_ENV=production npm run serve",
    "build": "npm run build-ts && npm run lint",
    "upload": "gulp buildPrd",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "cross-env NODE_ENV=development concurrently -k -p '[{name}]' -n 'TypeScript,Node' -c 'bold,cyan.bold,green.bold' 'npm run watch-ts' 'npm run watch-node'",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint '**/*.{js,ts}' --quiet --fix",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p '[{name}]' -n 'TypeScript,Node' -c 'bold,cyan.bold,green.bold' 'npm run watch-ts' 'npm run serve-debug'"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "./dist/*.js.map",
      "./dist/*/*.js.map"
    ],
    "verbose": false,
    "execMap": {
      "js": "node"
    },
    "watch": [
      "./dist"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js"
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "@types/express-session": "^1.17.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "cheerio": "^1.0.0-rc.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.0-rc.0",
    "colorthief": "^2.3.2",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.1",
    "express": "^4.17.1",
    "express-errors": "^0.3.1",
    "express-promise-router": "^3.0.3",
    "express-winston": "^4.0.5",
    "fs-extra": "^9.0.1",
    "gm": "^1.23.1",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "http-errors": "^1.7.3",
    "ico-to-png": "^0.2.1",
    "jest": "^25.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "mongoose": "^5.9.6",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "redis": "^3.0.2",
    "svg-captcha": "^1.4.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/cheerio": "^0.22.18",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/eslint": "^6.1.8",
    "@types/express": "^4.17.3",
    "@types/express-winston": "^4.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/gm": "^1.18.9",
    "@types/helmet": "0.0.45",
    "@types/hpp": "^0.2.1",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.157",
    "@types/mongoose": "^5.7.32",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.3",
    "@types/node": "^12.12.31",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/redis": "^2.8.17",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "async": "^3.2.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-ssh": "^0.7.0",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.0.1",
    "prettier": "^2.0.2",
    "progress": "^2.0.3",
    "scp2": "^0.5.0",
    "supertest": "^4.0.2",
    "through2": "^3.0.1"
  }
}
